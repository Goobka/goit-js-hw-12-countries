<ul>
{{#each this}}
<li>
    <h2>{{name}}</h2>
    <img src="{{flag}}" alt="flag" width="320">
    <p>Capital: {{capital}}</p>
    <p>Population: {{population}}</p>
    <ul>
        <p>Languages:</p>
        {{#each languages}}
        <li>{{this.name}}</li>
        {{/each}}
    </ul>
</li>
{{/each}}
</ul>